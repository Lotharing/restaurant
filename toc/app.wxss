/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.primaryText{
    color: rgba(0, 0, 0, 0.87);
    font-size: 18px;

}
.secondaryText{
    color: rgba(0, 0, 0, 0.87);
    font-size: 15px;
}
.disabledText{
    color: rgba(0, 0, 0, 0.70);
    font-size: 15px;
}
.actionBar{
    background: rgb(0,105,92);
}
.button{
    background: rgb(255,214,0);
}
.personal{
    float: right;
    display: flex;
    align-items: center;
    justify-content:center;
    font-size: 20px;
    color: #fff;
    height: 100%;
    margin-right: 10px;
}
::-webkit-scrollbar{
    width: 0;
    height: 0;
    color: transparent;
}
